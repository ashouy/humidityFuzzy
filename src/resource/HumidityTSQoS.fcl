DEFUZZIFY umidade

	METHOD : COG;
	DEFAULT := 0;

END_DEFUZZIFY

RULEBLOCK No1

	ACCU : MAX;
	AND : MIN;

	RULE 1 : IF fase IS fase1 AND sensor1 IS baixo THEN umidade IS seco;

END_RULEBLOCK

END_FUNCTION_BLOCK