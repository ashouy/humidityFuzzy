FUNCTION_BLOCK TSQoSFewRules

VAR_INPUT
	sensor1 : REAL;
	sensor2 : REAL;
	sensor3 : REAL;
	fase : REAL;
END_VAR

VAR_OUTPUT
	umidade : REAL;
END_VAR

FUZZIFY sensor1
	TERM baixo := (250,1) (327,1) (405,0);
	TERM medio := (327,0) (405,1) (482,0);
	TERM alto := (405,0) (482,1) (560,1);
END_FUZZIFY

FUZZIFY sensor2
	TERM baixo := (250,1) (327,1) (405,0);
	TERM medio := (327,0) (405,1) (482,0);
	TERM alto := (405,0) (482,1) (560,1);
